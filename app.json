{
  "expo": {
    "name": "Lub1Bfv",
    "slug": "projetoLub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#35AAFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bfv.lub1bfv"
    },
    "android": {
      "package": "com.bfv.lub1bfv",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Projeto de controle de frota e lubrificação para projeto de TCC.",
    "githubUrl": "https://github.com/liciomachado/lub-1bfv-api/tree/master"
  }
}
